doctype html
html(lang='en' class='no-js')
  block vars
    - var headerClass = '';
    - var contentClass = '';
    - var footerClass = '';
  
  include _head
  
  body
    include _header
    include _content
    include _footer
  
  //- Vue.js -- development
  script(src='https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.js')
  
  //- Vue.js --production
  //- script(src='https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.min.js')
  
  //- Quasar
  script(src='https://cdn.jsdelivr.net/npm/quasar-framework@latest/dist/umd/quasar.ios.umd.min.js')
  
  //- Main Application JS
  script(src='/assets/scripts/app-min.js')
